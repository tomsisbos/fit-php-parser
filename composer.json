{
    "name": "arnaud-deabreu/fit-php-parser",
    "type": "library",
    "description": "Unofficial PHP library to parse FIT files",
    "keywords": ["fit"],
    "homepage": "https://nodelia.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud DE ABREU",
            "email": "contact@nodelia.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/string": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "FitParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FitParser\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3 || ^12.0",
        "symfony/var-dumper": "^7.1",
        "symfony/console": "^7.1",
        "phpoffice/phpspreadsheet": "^2.2 || ^3.0 || ^4.0",
        "nette/php-generator": "^4.1",
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.62"
    }
}
